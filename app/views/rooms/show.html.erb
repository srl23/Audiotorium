<h1><%= @room.name %></h1>
<% if current_user == @room.user %>
  <%= link_to "Edit", edit_room_path(@room) %>
  <br/>
<% end %>
Currently Playing.. <br/> <%= @room.song_file %> <br/>
<%= audio_tag(@room.song_file, autoplay: true, controls: true) %>
<div style="width: 50%; margin-left: auto; margin-right: auto; border: 1px solid black;">
  <div  id="messages" data-room-id="<%= @room.id %>">
    <%= render @room.messages.order(created_at: :desc) %>
  </div>
</div>
<form>
  <input id='send_box' type=text'/>
</form>
